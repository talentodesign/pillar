<IfModule mod_rewrite.c>
RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d 
RewriteCond %{REQUEST_FILENAME}\.php -f 
RewriteRule ^quem-somos/?$ quem-somos.php
RewriteRule ^fale-conosco/?$ fale-conosco.php
RewriteRule ^empreendimentos/?$ emp.php
RewriteRule ^obras-andamento/?$ and-index.php
RewriteRule ^obras-concluidas/?$ conc-index.php
RewriteRule ^obra-a/?$ obra-a.php
RewriteRule ^parametro_string/pg/([a-z0-9-]*)\/?$ parametro_string.php?pg=$1
RewriteRule ^parametro_int\/?([1-9]*)\/?$ parametro_int.php?qs_pg2805=$1
</IfModule>
